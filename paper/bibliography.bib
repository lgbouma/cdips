@article{livingston_three_2018,
	title = {Three {Small} {Planets} {Transiting} a {Hyades} {Star}},
	volume = {155},
	issn = {1538-3881},
	url = {https://doi.org/10.3847%2F1538-3881%2Faaa841},
	doi = {10.3847/1538-3881/aaa841},
	abstract = {We present the discovery of three small planets transiting K2-136 (LP 358 348, EPIC 247589423), a late K dwarf in the Hyades. The planets have orbital periods of 7.9757 ± 0.0011, , and , and radii of 1.05 ± 0.16, 3.14 ± 0.36, and , respectively. With an age of 600–800 Myr, these planets are some of the smallest and youngest transiting planets known. Due to the relatively bright (J = 9.1) host star, the planets are compelling targets for future characterization via radial velocity mass measurements and transmission spectroscopy. As the first known star with multiple transiting planets in a cluster, the system should be helpful for testing theories of planet formation and migration.},
	language = {en},
	number = {3},
	urldate = {2019-06-13},
	journal = {The Astronomical Journal},
	author = {Livingston, John H. and Dai, Fei and Hirano, Teruyuki and Gandolfi, Davide and Nowak, Grzegorz and Endl, Michael and Velasco, Sergio and Fukui, Akihiko and Narita, Norio and Prieto-Arranz, Jorge and Barragan, Oscar and Cusano, Felice and Albrecht, Simon and Cabrera, Juan and Cochran, William D. and Csizmadia, Szilard and Deeg, Hans J. and Eigmüller, Philipp and Erikson, Anders and Fridlund, Malcolm and Grziwa, Sascha and Guenther, Eike W. and Hatzes, Artie P. and Kawauchi, Kiyoe and Korth, Judith and Nespral, David and Palle, Enric and Pätzold, Martin and Persson, Carina M. and Rauer, Heike and Smith, Alexis M. S. and Tamura, Motohide and Tanaka, Yusuke and Eylen, Vincent Van and Watanabe, Noriharu and Winn, Joshua N.},
