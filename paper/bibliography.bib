@article{cantat-gaudin_gaia_2018,
	title = {A {Gaia} {DR}2 view of the open cluster population in the {Milky} {Way}},
	volume = {618},
	copyright = {© ESO 2018},
	issn = {0004-6361, 1432-0746},
	url = {https:/articles/aa/abs/2018/10/aa33476-18/aa33476-18.html},
	doi = {10.1051/0004-6361/201833476},
	abstract = {{\textless}i{\textgreater}Context.{\textless}i/{\textgreater} Open clusters are convenient probes of the structure and history of the Galactic disk. They are also fundamental to stellar evolution studies. The second {\textless}i{\textgreater}Gaia{\textless}i/{\textgreater} data release contains precise astrometry at the submilliarcsecond level and homogeneous photometry at the mmag level, that can be used to characterise a large number of clusters over the entire sky.{\textless}i{\textgreater}Aims.{\textless}i/{\textgreater} In this study we aim to establish a list of members and derive mean parameters, in particular distances, for as many clusters as possible, making use of {\textless}i{\textgreater}Gaia{\textless}i/{\textgreater} data alone.{\textless}i{\textgreater}Methods.{\textless}i/{\textgreater} We compiled a list of thousands of known or putative clusters from the literature. We then applied an unsupervised membership assignment code, UPMASK, to the {\textless}i{\textgreater}Gaia{\textless}i/{\textgreater} DR2 data contained within the fields of those clusters.{\textless}i{\textgreater}Results.{\textless}i/{\textgreater} We obtained a list of members and cluster parameters for 1229 clusters. As expected, the youngest clusters are seen to be tightly distributed near the Galactic plane and to trace the spiral arms of the Milky Way, while older objects are more uniformly distributed, deviate further from the plane, and tend to be located at larger Galactocentric distances. Thanks to the quality of {\textless}i{\textgreater}Gaia{\textless}i/{\textgreater} DR2 astrometry, the fully homogeneous parameters derived in this study are the most precise to date. Furthermore, we report on the serendipitous discovery of 60 new open clusters in the fields analysed during this study.},
	language = {en},
	urldate = {2019-03-06},
	journal = {Astronomy \& Astrophysics},
	author = {Cantat-Gaudin, T. and Jordi, C. and Vallenari, A. and Bragaglia, A. and Balaguer-Núñez, L. and Soubiran, C. and Bossini, D. and Moitinho, A. and Castro-Ginard, A. and Krone-Martins, A. and Casamiquela, L. and Sordo, R. and Carrera, R.},
	month = oct,
	year = {2018},
	pages = {A93},
	file = {Full Text PDF:/home/luke/local/Zotero/storage/CA8ICTDD/Cantat-Gaudin et al. - 2018 - A Gaia DR2 view of the open cluster population in .pdf:application/pdf;Snapshot:/home/luke/local/Zotero/storage/EBECUSCK/aa33476-18.html:text/html}
}

