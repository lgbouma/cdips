@article{roser_deep_2011,
	title = {A deep all-sky census of the {Hyades}},
	volume = {531},
	copyright = {© ESO, 2011},
	issn = {0004-6361, 1432-0746},
	url = {https://www.aanda.org/articles/aa/abs/2011/07/aa16948-11/aa16948-11.html},
	doi = {10.1051/0004-6361/201116948},
	abstract = {{\textless}i{\textgreater}Aims.{\textless}i/{\textgreater} On the basis of the PPMXL catalogue we perform an all-sky census of the Hyades down to masses of about 0.2 {\textless}i{\textgreater}m{\textless}i/{\textgreater}{\textless}sub{\textgreater}⊙{\textless}sub/{\textgreater} in a region up to 30 pc from the cluster centre.{\textless}i{\textgreater}Methods.{\textless}i/{\textgreater} We use the proper motions from PPMXL in the convergent point method to determine probable kinematic members. From 2MASS photometry and CMC14 {\textless}i{\textgreater}r′{\textless}i/{\textgreater}-band photometry, we derive empirical colour-absolute magnitude diagrams and, finally, determine photometric membership for all kinematic candidates. {\textless}i{\textgreater}Results.{\textless}i/{\textgreater} This is the first deep ({\textless}i{\textgreater}r′{\textless}i/{\textgreater} ≤ 17) all-sky survey of the Hyades allowing a full three-dimensional analysis of the cluster. The survey is complete down to at least {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}{\textless}i{\textgreater}K{\textless}i/{\textgreater}{\textless}sub{\textgreater}s{\textless}sub/{\textgreater}{\textless}sub/{\textgreater} = 7.3 or 0.25 {\textless}i{\textgreater}m{\textless}i/{\textgreater}{\textless}sub{\textgreater}⊙{\textless}sub/{\textgreater}. We find 724 stellar systems co-moving with the bulk Hyades space velocity, which represent a total mass of 435 {\textless}i{\textgreater}m{\textless}i/{\textgreater}{\textless}sub{\textgreater}⊙{\textless}sub/{\textgreater}. The tidal radius is about 9 pc, and 275 {\textless}i{\textgreater}m{\textless}i/{\textgreater}{\textless}sub{\textgreater}⊙{\textless}sub/{\textgreater} (364 systems) are gravitationally bound. This is the cluster proper. Its mass density profile is perfectly fitted by a Plummer model with a central density of 2.21 {\textless}i{\textgreater}m{\textless}i/{\textgreater}{\textless}sub{\textgreater}⊙{\textless}sub/{\textgreater} pc{\textless}sup{\textgreater}-3{\textless}sup/{\textgreater} and a core radius of {\textless}i{\textgreater}r{\textless}i/{\textgreater}{\textless}sub{\textgreater}co{\textless}sub/{\textgreater} = 3.10 pc, while the half-mass radius is {\textless}i{\textgreater}r{\textless}i/{\textgreater}{\textless}sub{\textgreater}h{\textless}sub/{\textgreater} = 4.1 pc. There are another 100 {\textless}i{\textgreater}m{\textless}i/{\textgreater}{\textless}sub{\textgreater}⊙{\textless}sub/{\textgreater} in a volume between one and two tidal radii (halo), and another 60 {\textless}i{\textgreater}m{\textless}i/{\textgreater}{\textless}sub{\textgreater}⊙{\textless}sub/{\textgreater} up to a distance of 30 pc from the centre. Strong mass segregation is inherent in the cluster. The present-day luminosity and mass functions are noticeably different in various parts of the cluster (core, corona, halo, and co-movers). They are strongly evolved compared to presently favoured initial mass functions. The analysis of the velocity dispersion of the cluster shows that about 20\% of its members must be binaries. As a by-product, we find that presently available theoretical isochrones are not able to adequately describe the near-infrared colour-absolute magnitude relation for those cluster stars that are less massive than about 0.6 {\textless}i{\textgreater}m{\textless}i/{\textgreater}{\textless}sub{\textgreater}⊙{\textless}sub/{\textgreater}.},
	language = {en},
	urldate = {2019-03-06},
	journal = {Astronomy \& Astrophysics},
	author = {R\"oser, S. and Schilbach, E. and Piskunov, A. E. and Kharchenko, N. V. and Scholz, R.-D.},
	month = jul,
	year = {2011},
	pages = {A92},
	file = {Full Text PDF:/home/luke/local/Zotero/storage/7J9AB6QV/R\"oser et al. - 2011 - A deep all-sky census of the Hyades.pdf:application/pdf}
}

@article{rizzuto_multidimensional_2011,
