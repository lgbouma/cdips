	file = {NASA/ADS Full Text PDF:/Users/luke/Zotero/storage/I3DH65CH/Irwin et al. - 2007 - The Monitor project data processing and light cur.pdf:application/pdf}
}

@article{aigrain_monitor_2007,
	title = {The {Monitor} project: searching for occultations in young open clusters},
	volume = {375},
	issn = {0035-8711},
	shorttitle = {The {Monitor} project},
	url = {http://adsabs.harvard.edu/abs/2007MNRAS.375...29A},
	doi = {10.1111/j.1365-2966.2006.11303.x},
	abstract = {The Monitor project is a photometric monitoring survey of nine young (1-200Myr) clusters in the solar neighbourhood to search for eclipses by very low mass stars and brown dwarfs and for planetary transits in the light curves of cluster members. It began in the autumn of 2004 and uses several 2- to 4-m telescopes worldwide. We aim to calibrate the relation between age, mass, radius and where possible luminosity, from the K dwarf to the planet regime, in an age range where constraints on evolutionary models are currently very scarce. Any detection of an exoplanet in one of our youngest targets ({\textless}{\textasciitilde}10Myr) would also provide important constraints on planet formation and migration time-scales and their relation to protoplanetary disc lifetimes. Finally, we will use the light curves of cluster members to study rotation and flaring in low-mass pre-main-sequence stars.
	The present paper details the motivation, science goals and observing strategy of the survey. We present a method to estimate the sensitivity and number of detections expected in each cluster, using a simple semi-analytic approach which takes into account the characteristics of the cluster and photometric observations, using (tunable) best-guess assumptions for the incidence and parameter distribution of putative companions, and we incorporate the limits imposed by radial velocity follow-up from medium and large telescopes. We use these calculations to show that the survey as a whole can be expected to detect over 100 young low and very low mass eclipsing binaries, and {\textasciitilde}3 transiting planets with radial velocity signatures detectable with currently available
facilities.},
	urldate = {2017-09-18},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Aigrain, S. and Hodgkin, S. and Irwin, J. and Hebb, L. and Irwin, M. and Favata, F. and Moraux, E. and Pont, F.},
	month = feb,
	year = {2007},
	keywords = {planetary systems, stars: low-mass, binaries: eclipsing, brown dwarfs, stars: pre-main-sequence, occultations},
	pages = {29--52},
	file = {NASA/ADS Full Text PDF:/Users/luke/Zotero/storage/5PULZXWG/Aigrain et al. - 2007 - The Monitor project searching for occultations in.pdf:application/pdf}
}


@article{hartman_MMT_IV_2009,
	title = {Deep {MMT} {Transit} {Survey} of the {Open} {Cluster} {M}37 {IV}: {Limit} on the {Fraction} of {Stars} with {Planets} as {Small} as 0.3R{\textless}{SUB}{\textgreater}{J}{\textless}/{SUB}{\textgreater}},
	volume = {695},
	shorttitle = {Deep {MMT} {Transit} {Survey} of the {Open} {Cluster} {M}37 {IV}},
	url = {https://ui.adsabs.harvard.edu/abs/2009ApJ...695..336H/abstract},
	doi = {10.1088/0004-637X/695/1/336},
	abstract = {We present the results of a deep (15 lsim r lsim 23), 20 night survey for transiting planets in the intermediate-age open cluster M37 (NGC 2099) using the Megacam wide-field mosaic CCD camera on the 6.5 m MMT. We do not detect any transiting planets among the {\textasciitilde}1450 observed cluster members. We do, however, identify a {\textasciitilde}1R{\textless}SUB{\textgreater}J{\textless}/SUB{\textgreater} candidate planet transiting a {\textasciitilde}0.8 M {\textless}SUB{\textgreater}sun{\textless}/SUB{\textgreater} Galactic field star with a period of 0.77 days. The source is faint (V = 19.85 mag) and has an expected velocity semiamplitude of K {\textasciitilde} 220 m s{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater}(M/M{\textless}SUB{\textgreater}J{\textless}/SUB{\textgreater} ). We conduct Monte Carlo transit injection and recovery simulations to calculate the 95\% confidence upper limit on the fraction of cluster members and field stars with planets as a function of planetary radius and orbital period. Assuming a uniform logarithmic distribution in the orbital period, we find that \&lt;1.1\%, \&lt;2.7\%, and \&lt;8.3\% of cluster members have 1.0R{\textless}SUB{\textgreater}J{\textless}/SUB{\textgreater} planets within extremely hot Jupiter (EHJ; 0.4 \&lt; P \&lt; 1.0 day), very hot Jupiter (VHJ; 1.0 \&lt; P \&lt; 3.0 day), and hot Jupiter (HJ; 3.0 \&lt; P \&lt; 5.0 day) period ranges, respectively. For 0.5R{\textless}SUB{\textgreater}J{\textless}/SUB{\textgreater} planets, the limits are less than 3.2\% and less than 21\% for EHJ and VHJ period ranges, respectively, while for 0.35R{\textless}SUB{\textgreater}J{\textless}/SUB{\textgreater} planets we can only place an upper limit of less than 25\% on the EHJ period range. For a sample of 7814 Galactic field stars, consisting primarily of FGKM dwarfs, we place 95\% upper limits of \&lt;0.3\%, \&lt;0.8\%, and \&lt;2.7\% on the fraction of stars with a 1.0R{\textless}SUB{\textgreater}J{\textless}/SUB{\textgreater} EHJ, VHJ, and HJ, respectively, assuming that the candidate planet is not genuine. If the candidate is genuine, the frequency of {\textasciitilde}1.0R{\textless}SUB{\textgreater}J{\textless}/SUB{\textgreater} planets in the EHJ period range is 0.002\% \&lt; f {\textless}SUB{\textgreater}EHJ{\textless}/SUB{\textgreater} \&lt; 0.5\% with 95\% confidence. We place limits of \&lt;1.4\%, \&lt;8.8\%, and \&lt;47\% for 0.5R{\textless}SUB{\textgreater}J{\textless}/SUB{\textgreater} planets, and a limit of \&lt;16\% on 0.3R{\textless}SUB{\textgreater}J{\textless}/SUB{\textgreater} planets in the EHJ period range. This is the first transit survey to place limits on the fraction of stars with planets as small as Neptune. Observations reported here were obtained at the MMT Observatory, a joint facility of the Smithsonian Institution and the University of Arizona.},
	language = {en},
	number = {1},
	urldate = {2019-06-12},
	journal = {The Astrophysical Journal},
	author = {Hartman, J. D. and Gaudi, B. S. and Holman, M. J. and McLeod, B. A. and Stanek, K. Z. and Barranco, J. A. and Pinsonneault, M. H. and Meibom, S. and Kalirai, J. S.},
	month = apr,
	year = {2009},
	pages = {336},
	file = {Full Text PDF:/Users/luke/Zotero/storage/JXABBCV8/Hartman et al. - 2009 - Deep MMT Transit Survey of the Open Cluster M37 IV.pdf:application/pdf;Snapshot:/Users/luke/Zotero/storage/HP9CP3X9/abstract.html:text/html}
}

