	url = {https://ui.adsabs.harvard.edu//#abs/2018AJ....155..222D/abstract},
	doi = {10.3847/1538-3881/aabde8},
	abstract = {We report the detection of three small transiting planets around the young K3 dwarf K2-233 (2MASS J15215519-2013539) from observations during Campaign 15 of the K2 mission. The star is relatively nearby (d = 69 pc) and bright (V = 10.7 mag, K {\textless}SUB{\textgreater} s {\textless}/SUB{\textgreater} = 8.4 mag), making the planetary system an attractive target for radial velocity follow-up and atmospheric characterization with the James Webb Space Telescope. The inner two planets are hot super-Earths (R {\textless}SUB{\textgreater} b {\textless}/SUB{\textgreater} = 1.40 ± 0.06 \{R\}{\textless}SUB{\textgreater}{\textbackslash}oplus {\textless}/SUB{\textgreater}, R {\textless}SUB{\textgreater} c {\textless}/SUB{\textgreater} = 1.34 ± 0.08 \{R\}{\textless}SUB{\textgreater}{\textbackslash}oplus {\textless}/SUB{\textgreater}), while the outer planet is a warm sub-Neptune (R {\textless}SUB{\textgreater} d {\textless}/SUB{\textgreater} = 2.6 ± 0.1 \{R\}{\textless}SUB{\textgreater}{\textbackslash}oplus {\textless}/SUB{\textgreater}). We estimate the stellar age to be \{360\}{\textless}SUB{\textgreater}-140{\textless}/SUB{\textgreater}{\textless}SUP{\textgreater}+490{\textless}/SUP{\textgreater} Myr based on rotation, activity, and kinematic indicators. The K2-233 system is particularly interesting given recent evidence for inflated radii in planets around similarly aged stars, a trend potentially related to photo-evaporation, core cooling, or both mechanisms.},
	language = {en},
	number = {5},
	urldate = {2019-01-28},
	journal = {The Astronomical Journal},
	author = {David, Trevor J. and Crossfield, Ian J. M. and Benneke, Björn and Petigura, Erik A. and Gonzales, Erica J. and Schlieder, Joshua E. and Yu, Liang and Isaacson, Howard T. and Howard, Andrew W. and Ciardi, David R. and Mamajek, Eric E. and Hillenbrand, Lynne A. and Cody, Ann Marie and Riedel, Adric and Schwengeler, Hans Martin and Tanner, Christopher and Ende, Martin},
	month = may,
	year = {2018},
	pages = {222},
	file = {Full Text PDF:/home/luke/local/Zotero/storage/BYQHGTFG/David et al. - 2018 - Three Small Planets Transiting the Bright Young Fi.pdf:application/pdf;Snapshot:/home/luke/local/Zotero/storage/STHR4QE2/ui.adsabs.harvard.edu.html:text/html}
}

@article{david_v1298_2019,
	title = {A warm {Jupiter}-sized planet transiting the pre-main sequence star {V}1298 {Tau}},
	url = {http://arxiv.org/abs/1902.09670},
	abstract = {We report the detection of V1298 Tau b, a warm Jupiter-sized planet (\$R\_P\$ = 0.91 \${\textbackslash}pm\$ 0.05{\textasciitilde} \$R\_{\textbackslash}mathrm\{Jup\}\$, \$P = 24.1\$ days) transiting a young solar analog with an estimated age of 23 million years. The star and its planet belong to Group 29, a young association in the foreground of the Taurus-Auriga star-forming region. While hot Jupiters have been previously reported around young stars, those planets are non-transiting and near-term atmospheric characterization is not feasible. The V1298 Tau system is a compelling target for follow-up study through transmission spectroscopy and Doppler tomography owing to the transit depth (0.5{\textbackslash}\%), host star brightness (\$K\_s\$ = 8.1 mag), and rapid stellar rotation (\$v{\textbackslash}sin\{i\}\$ = 23 {\textbackslash}kms). Although the planet is Jupiter-sized, its mass is presently unknown due to high-amplitude radial velocity jitter. Nevertheless, V1298 Tau b may help constrain formation scenarios for at least one class of close-in exoplanets, providing a window into the nascent evolution of planetary interiors and atmospheres.},
	urldate = {2019-06-13},
	journal = {arXiv:1902.09670 [astro-ph]},
	author = {David, Trevor J. and Cody, Ann Marie and Hedges, Christina L. and Mamajek, Eric E. and Hillenbrand, Lynne A. and Ciardi, David R. and Beichman, Charles A. and Petigura, Erik A. and Fulton, Benjamin J. and Isaacson, Howard T. and Howard, Andrew W. and Gagné, Jonathan and Saunders, Nicholas K. and Rebull, Luisa M. and Stauffer, John R. and Vasisht, Gautam and Hinkley, Sasha},
	month = feb,
	year = {2019},
	note = {arXiv: 1902.09670},
	keywords = {Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Earth and Planetary Astrophysics},
	annote = {Comment: Accepted to AJ},
	file = {arXiv\:1902.09670 PDF:/home/luke/local/Zotero/storage/66F2QUEN/David et al. - 2019 - A warm Jupiter-sized planet transiting the pre-mai.pdf:application/pdf}
}

@article{rizzuto_zeitVIII_2018,
	title = {Zodiacal {Exoplanets} in {Time} ({ZEIT}) {VIII}: {A} {Two} {Planet} {System} in {Praesepe} from {K}2 {Campaign} 16},
	shorttitle = {Zodiacal {Exoplanets} in {Time} ({ZEIT}) {VIII}},
	url = {http://arxiv.org/abs/1808.07068},
	abstract = {Young planets offer a direct view of the the formation and evolution processes that produced the diverse population of mature exoplanet systems known today. The repurposed Kepler mission K2 is providing the first sample of young transiting planets by observing populations of stars in nearby, young clusters or stellar associations. We report the detection and confirmation of two planets transiting EPIC 211964830, an M2.5 dwarf in the 650 Myr old Praesepe open cluster. Using our notch-filter search method on the K2 lightcurve, we identify planets with periods of 5.84 d and 19.66 d. This is currently the second known multi-transit system in open clusters younger than 1 Gyr. The inner planet has a radius of 2.27\$\_\{-0.16\}{\textasciicircum}\{+0.20\}\$ R\$\_{\textbackslash}oplus\$ and the outer planet has a radius of 2.77\$\_\{-0.18\}{\textasciicircum}\{+0.20\}\$ R\$\_{\textbackslash}oplus\$. Both planets are likely mini-Neptunes. These planets are expected to produce radial velocity signals of 3.4 and 2.7 m/s respectively, which is smaller than the expected stellar variability in the optical (\${\textbackslash}simeq\$30{\textbackslash},m/s), making mass measurements unlikely in the optical, but possible with future near-infrared spectrographs. We use an injection-recovery test to place robust limits on additional planets in the system, and find that planets larger than 1-2 R\$\_{\textbackslash}oplus\$ with periods of 1-20 d are unlikely, while planets larger than 2 R\$\_{\textbackslash}oplus\$ are ruled out at periods of 20-30 d.},
	urldate = {2018-08-23},
	journal = {arXiv:1808.07068 [astro-ph]},
	author = {Rizzuto, Aaron C. and Vanderburg, Andrew and Mann, Andrew W. and Kraus, Adam L. and Dressing, Courtney D. and Agüeros, Marcel A. and Douglas, Stephanie T.},
	month = aug,
	year = {2018},
	note = {arXiv: 1808.07068},
	keywords = {Astrophysics - Earth and Planetary Astrophysics}
}

@article{mann_ZEITVI_2018,
	title = {Zodiacal {Exoplanets} in {Time} ({ZEIT}). {VI}. {A} {Three}-planet {System} in the {Hyades} {Cluster} {Including} an {Earth}-sized {Planet}},
	volume = {155},
	url = {https://ui.adsabs.harvard.edu/abs/2018AJ....155....4M/abstract},
	doi = {10.3847/1538-3881/aa9791},
	abstract = {Planets in young clusters are powerful probes of the evolution of planetary systems. Here we report the discovery of three planets transiting EPIC 247589423, a late-K dwarf in the Hyades (≃800 Myr) cluster, and robust detection limits for additional planets in the system. The planets were identified from their K2 light curves as part of our survey of young clusters and star-forming regions. The smallest planet has a radius comparable to Earth (\{0.99\}{\textless}SUB{\textgreater}-0.04{\textless}/SUB{\textgreater}{\textless}SUP{\textgreater}+0.06{\textless}/SUP{\textgreater}\{R\}{\textless}SUB{\textgreater}{\textbackslash}oplus {\textless}/SUB{\textgreater}), making it one of the few Earth-sized planets with a known, young age. The two larger planets are likely a mini-Neptune and a super-Earth, with radii of \{2.91\}{\textless}SUB{\textgreater}-0.10{\textless}/SUB{\textgreater}{\textless}SUP{\textgreater}+0.11{\textless}/SUP{\textgreater}\{R\}{\textless}SUB{\textgreater}{\textbackslash}oplus {\textless}/SUB{\textgreater} and \{1.45\}{\textless}SUB{\textgreater}-0.08{\textless}/SUB{\textgreater}{\textless}SUP{\textgreater}+0.11{\textless}/SUP{\textgreater}\{R\}{\textless}SUB{\textgreater}{\textbackslash}oplus {\textless}/SUB{\textgreater}, respectively. The predicted radial velocity signals from these planets are between 0.4 and 2 m s{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater}, achievable with modern precision RV spectrographs. Because the target star is bright (V = 11.2) and has relatively low-amplitude stellar variability for a young star (2-6 mmag), EPIC 247589423 hosts the best known planets in a young open cluster for precise radial velocity follow-up, enabling a robust test of earlier claims that young planets are less dense than their older counterparts.},
	language = {en},
	number = {1},
	urldate = {2019-06-13},
	journal = {The Astronomical Journal},
	author = {Mann, Andrew W. and Vanderburg, Andrew and Rizzuto, Aaron C. and Kraus, Adam L. and Berlind, Perry and Bieryla, Allyson and Calkins, Michael L. and Esquerdo, Gilbert A. and Latham, David W. and Mace, Gregory N. and Morris, Nathan R. and Quinn, Samuel N. and Sokal, Kimberly R. and Stefanik, Robert P.},
	month = jan,
	year = {2018},
	pages = {4},
	file = {Full Text PDF:/home/luke/local/Zotero/storage/8WTHFEBM/Mann et al. - 2018 - Zodiacal Exoplanets in Time (ZEIT). VI. A Three-pl.pdf:application/pdf;Snapshot:/home/luke/local/Zotero/storage/BX769L9X/abstract.html:text/html}
}

