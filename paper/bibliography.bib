	pages = {A144},
	file = {Full Text PDF:/home/luke/local/Zotero/storage/FJ7PBZAC/Marrese et al. - 2019 - Gaia Data Release 2 - Cross-match with external ca.pdf:application/pdf;Snapshot:/home/luke/local/Zotero/storage/SK7KFHZ3/aa34142-18.html:text/html}
}

