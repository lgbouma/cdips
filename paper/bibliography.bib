	author = {Gilliland, Ronald L. and Brown, T. M. and Guhathakurta, P. and Sarajedini, A. and Milone, E. F. and Albrow, M. D. and Baliber, N. R. and Bruntt, H. and Burrows, A. and Charbonneau, D. and Choi, P. and Cochran, W. D. and Edmonds, P. D. and Frandsen, S. and Howell, J. H. and Lin, D. N. C. and Marcy, G. W. and Mayor, M. and Naef, D. and Sigurdsson, S. and Stagg, C. R. and Vandenberg, D. A. and Vogt, S. S. and Williams, M. D.},
	year = {2000},
	pages = {L47},
	file = {IOP Full Text PDF:/Users/luke/Zotero/storage/KAQN32IV/Gilliland et al. - 2000 - A Lack of Planets in 47 Tucanae from a Hubble Spac.pdf:application/pdf}
}

@article{howell_k2_2014,
	title = {The {K}2 {Mission}: {Characterization} and {Early} {Results}},
	volume = {126},
	issn = {1538-3873},
	shorttitle = {The {K}2 {Mission}},
