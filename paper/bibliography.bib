	pages = {3108--3117}
}

@article{bell_32ori_2017,
	title = {A stellar census of the nearby, young 32 {Orionis} group},
	volume = {468},
	issn = {0035-8711},
	url = {http://adsabs.harvard.edu/abs/2017MNRAS.468.1198B},
	doi = {10.1093/mnras/stx535},
	abstract = {The 32 Orionis group was discovered almost a decade ago and despite the fact that it represents the first northern, young (age ˜ 25 Myr) stellar aggregate within 100 pc of the Sun (d ≃ 93 pc), a
comprehensive survey for members and detailed characterization of the group has yet to be performed. We present the first large-scale
spectroscopic survey for new (predominantly M type) members of the group after combining kinematic and photometric data to select candidates with Galactic space motion and positions in colour-magnitude space consistent with membership. We identify 30 new members, increasing the number of known 32 Ori group members by a factor of 3 and bringing the total number of identified members to 46, spanning spectral types B5 to L1. We also identify the lithium depletion boundary (LDB) of the group, I.e. the luminosity at which lithium remains unburnt in a coeval population. We estimate the age of the 32 Ori group independently using both isochronal fitting and LDB analyses and find it is essentially coeval with the β Pictoris moving group, with an age of 24 ± 4 Myr.
Finally, we have also searched for circumstellar disc hosts utilizing the AllWISE catalogue. Although we find no evidence for warm, dusty discs, we identify several stars with excess emission in the WISE W4 band at 22 μm. Based on the limited number of W4 detections, we
estimate a debris disc fraction of 32{\textasciicircum}\{+12\}\_\{-8\} per cent for the 32 Ori group.},
	urldate = {2019-03-06},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Bell, Cameron P. M. and Murphy, Simon J. and Mamajek, Eric E.},
	month = jun,
	year = {2017},
	keywords = {stars: pre-main-sequence, stars: fundamental parameters, open clusters and associations: general, stars: kinematics and dynamics, solar neighbourhood},
	pages = {1198--1220}
}

@article{oh_comoving_2017,
	title = {Comoving {Stars} in {Gaia} {DR}1: {An} {Abundance} of {Very} {Wide} {Separation} {Comoving} {Pairs}},
	volume = {153},
	issn = {1538-3881},
	shorttitle = {Comoving {Stars} in {Gaia} {DR}1},
	url = {http://stacks.iop.org/1538-3881/153/i=6/a=257},
	doi = {10.3847/1538-3881/aa6ffd},
	abstract = {The primary sample of the Gaia Data Release 1 is the Tycho- Gaia Astrometric Solution (TGAS): ≈2 million Tycho-2 sources with improved parallaxes and proper motions relative to the initial catalog. This increased astrometric precision presents an opportunity to find new binary stars and moving groups. We search for high-confidence comoving pairs of stars in TGAS by identifying pairs of stars consistent with having the same 3D velocity using a marginalized likelihood ratio test to discriminate candidate comoving pairs from the field population. Although we perform some visualizations using (bias-corrected) inverse parallax as a point estimate of distance, the likelihood ratio is computed with a probabilistic model that includes the covariances of parallax and proper motions and marginalizes the (unknown) true distances and 3D velocities of the stars. We find 13,085 comoving star pairs among 10,606 unique stars with separations as large as 10 pc (our search limit). Some of these pairs form larger groups through mutual comoving neighbors: many of these pair networks correspond to known open clusters and OB associations, but we also report the discovery of several new comoving groups. Most surprisingly, we find a large number of very wide ( \#\#IMG\#\# [http://ej.iop.org/images/1538-3881/153/6/257/ajaa6ffdieqn1.gif] \${\textbackslash}gt 1\$ pc) separation comoving star pairs, the number of which increases with increasing separation and cannot be explained purely by false-positive contamination. Our key result is a catalog of high-confidence comoving pairs of stars in TGAS. We discuss the utility of this catalog for making dynamical inferences about the Galaxy, testing stellar atmosphere models, and validating chemical abundance measurements.},
	language = {en},
	number = {6},
	journal = {The Astronomical Journal},
	author = {Oh, Semyeong and Price-Whelan, Adrian M. and Hogg, David W. and Morton, Timothy D. and Spergel, David N.},
	year = {2017},
	pages = {257},
	file = {IOP Full Text PDF:/home/luke/local/Zotero/storage/E58MBUH7/Oh et al. - 2017 - Comoving Stars in Gaia DR1 An Abundance of Very W.pdf:application/pdf}
}

@article{kraus_tucanahor_2014,
	title = {A {Stellar} {Census} of the {Tucana}-{Horologium} {Moving} {Group}},
	volume = {147},
	issn = {0004-6256},
	url = {http://adsabs.harvard.edu/abs/2014AJ....147..146K},
	doi = {10.1088/0004-6256/147/6/146},
	abstract = {We report the selection and spectroscopic confirmation of 129 new late-type (SpT = K3-M6) members of the Tucana-Horologium moving group, a nearby (d {\textasciitilde} 40 pc), young (τ {\textasciitilde} 40 Myr) population of comoving stars. We also report observations for 13 of the 17 known Tuc-Hor members in this spectral type range, and that 62 additional candidates are likely to be unassociated field stars; the confirmation frequency for new candidates is therefore 129/191 = 67\%. We have used radial velocities, Hα emission, and Li6708 absorption to distinguish
between contaminants and bona fide members. Our expanded census of Tuc-Hor increases the known population by a factor of {\textasciitilde}3 in total and by a factor of {\textasciitilde}8 for members with SpT {\textgreater}= K3, but even so, the K-M dwarf population of Tuc-Hor is still markedly incomplete. Our expanded census allows for a much more detailed study of Tuc-Hor than was previously feasible. The spatial distribution of members appears to trace a two-dimensional sheet, with a broad distribution in X and Y, but a very narrow distribution (±5 pc) in Z. The corresponding velocity
distribution is very small, with a scatter of ±1.1 km
s-1 about the mean UVW velocity for stars spanning the entire 50 pc extent of Tuc-Hor. We also show that the isochronal age (τ {\textasciitilde} 20-30 Myr) and the lithium depletion boundary age (τ {\textasciitilde} 40 Myr)
disagree, following the trend in other pre-main-sequence populations for isochrones to yield systematically younger ages. The Hα emission line strength follows a trend of increasing equivalent width with later spectral type, as is seen for young clusters. We find that moving group members have been depleted of measurable lithium for spectral types of K7.0-M4.5. None of our targets have significant infrared excesses in the WISE W3 band, yielding an upper limit on warm debris disks of F {\textless} 0.7\%. Finally, our purely kinematic and color-magnitude selection procedure allows us to test the efficiency and completeness for
activity-based selection of young stars. We find that 60\% of K-M dwarfs in Tuc-Hor do not have ROSAT counterparts and would have been omitted in X-ray-selected samples. In contrast, GALEX UV-selected samples using a previously suggested criterion for youth achieve completeness of 77\% and purity of 78\%, and we suggest new SpT-dependent selection criteria that will yield {\textgreater}95\% completeness for τ {\textasciitilde} 40 Myr populations with
GALEX data available.},
	urldate = {2019-03-06},
	journal = {The Astronomical Journal},
	author = {Kraus, Adam L. and Shkolnik, Evgenya L. and Allers, Katelyn N. and Liu, Michael C.},
	month = jun,
	year = {2014},
	keywords = {binaries: spectroscopic, open clusters and associations: individual: Tucana-Horologium, stars: activity, stars: kinematics and dynamics, stars: low-mass, stars: pre-main sequence},
	pages = {146},
	file = {Full Text PDF:/home/luke/local/Zotero/storage/FRQP5QIF/Kraus et al. - 2014 - A Stellar Census of the Tucana-Horologium Moving G.pdf:application/pdf}
}

@article{gagne_banyan_XII_2018,
	title = {{BANYAN}. {XII}. {New} {Members} of {Nearby} {Young} {Associations} from {GAIA}–{Tycho} {Data}},
	volume = {860},
